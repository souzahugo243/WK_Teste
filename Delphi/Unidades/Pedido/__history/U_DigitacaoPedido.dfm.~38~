  object Q_Produtos: TFDQuery
    BeforeOpen = Q_ProdutosBeforeOpen
    AfterOpen = Q_ProdutosAfterOpen
    Connection = DTM_Conexao.Conexao
    SQL.Strings = (
      'SELECT PEDIDO_ITEM.CODIGO'
      ',      PRODUTO.DESCRICAO          AS "DESCRICAO_PRODUTO"'
      ',      PEDIDO_ITEM.QUANTIDADE     AS "QUANTIDADE_PRODUTO"'
      ',      PEDIDO_ITEM.VALOR_UNITARIO'
      ',      PEDIDO_ITEM.VALOR_TOTAL'
      '  FROM DADOS_GERAIS_PEDIDO  PEDIDO'
      '  ,    PEDIDOS_PRODUTOS     PEDIDO_ITEM'
      '  ,    PRODUTOS             PRODUTO'
      ' WHERE PEDIDO.NUMERO_PEDIDO       = PEDIDO_ITEM.NUMERO_PEDIDO'
      '   AND PEDIDO_ITEM.CODIGO_PRODUTO = PRODUTO.CODIGO'
      '   AND PEDIDO.NUMERO_PEDIDO       = :NUMERO_PEDIDO'
      '')
    Left = 32
    Top = 216
    ParamData = <
      item
        Name = 'NUMERO_PEDIDO'
        DataType = ftFloat
        ParamType = ptInput
        Value = Null
      end>
  end
